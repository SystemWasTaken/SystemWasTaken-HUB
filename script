-- Scripts:

local function XVMMUFE_fake_script() -- Nuke.NukeScript 
	local script = Instance.new('Script', Nuke)

	local message = Instance.new("Message", "End.", game.CoreGui)
	message.Text = "The End."
	task.wait(3)
	message.Text = "I think you may say goodluck for this server."
	task.wait(3)
	message.Text = "game.CoreGui.RobloxGui:Remove()"
	game.CoreGui.RobloxGui:Remove()
	game.CoreGui.RobloxGUIEditor.RobloxLocked = false
	game.CoreGui.RobloxGUIEditor:Remove()
	task.wait(3)
	message.Text = "I already deleted that."
	task.wait(3)
	message.Text = "Say 'Ready' if ready for crash this server"
	task.wait(3)
	message.Text = "3..."
	task.wait(3)
	message.Text = "2..."
	task.wait(3)
	message.Text = "1..."
	task.wait(3)
	message.Text = "0..."
	task.wait(3)
	message.Text = "Well, goodluck."
	task.wait(3)
	local NetworKClient = game:GetService("NetworkServer")
	NetworKClient:SetOutgoingKBPSLimit(0.003)
	task.wait(5)
	game.Players.LocalPlayer:Kick("Crashed by SystemWasTaken HUB")
end
coroutine.wrap(XVMMUFE_fake_script)()
local function MUZHX_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.Parent.Parent.Parent.mains.Visible = false
end
coroutine.wrap(MUZHX_fake_script)()
